        $("#input_box").fadeOut(500, function(){
          $("#results_box").fadeIn(500, function(){
            $("#twitter_handle").text($("#input_twitter_handle").val())
            $.post("/analyze", { twitter_handle: $("#input_twitter_handle").val() }, handleReturns);
          })
        })
      }
      $("#zoomout").Morphist({
          animateIn: 'fadeInUp',
          animateOut: 'fadeOutUp',
          speed: 3000,
          complete: function(){
            if ($("#last_item").hasClass('animated')){
              $("#loading_text").text("OPERATOR CONTROL ACTIVE")
              $("#loading_libs").fadeOut(500, function(){
                $("#loading_box").animate({
                  height: 100
                }, 500);
                setTimeout(switchToSecondScreen, 1000);
              })
            }
          }
      });
      $("div > li").css("display", "flex");
  </script>
